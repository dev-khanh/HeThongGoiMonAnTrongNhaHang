<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="phuc.khanh.tinh.order">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="tinh.khanh.phuc.order.Login"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="tinh.khanh.phuc.order.phuc.Activity_NhaBep_Phuc"
            android:screenOrientation="landscape"/>
        <activity android:name="tinh.khanh.phuc.order.khanh.Khanh_Thu_Ngan"
            android:screenOrientation="landscape"/>
        <activity android:name="tinh.khanh.phuc.order.khanh.Khanh_Ban"
            android:screenOrientation="landscape"
            android:label="@string/Chon_Ban">
        </activity>
        <activity android:name="tinh.khanh.phuc.order.khanh.Khanh_Mon"
            android:screenOrientation="landscape"
            android:label="@string/Chon_Ban">
        </activity>

        <activity android:name="tinh.khanh.phuc.order.tinh.MainActivity_Tinh"
            android:screenOrientation="landscape"
            android:label="@string/Thu_Ngan">
        </activity>
    </application>

</manifest>